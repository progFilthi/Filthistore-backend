
generator client {
  provider = "prisma-client-js"
  // output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Beat{
  id String @id @default(cuid())
  title String
  key String
  bpm Int
  price Int
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
